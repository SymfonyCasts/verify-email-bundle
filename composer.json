{
    "name": "symfonycasts/verify-email-bundle",
    "description": "Simple, stylish Email Verification for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.3",
        "ext-json": "*",
        "symfony/config": "^6.4.5 | ^7.0",
        "symfony/dependency-injection": "^6.4.5 | ^7.0",
        "symfony/http-kernel": "^6.4.5 | ^7.0",
        "symfony/routing": "^6.4.5 | ^7.0",
        "symfony/deprecation-contracts": "^2.2 | ^3.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.7",
        "doctrine/persistence": "^2.0",
        "symfony/framework-bundle": "^6.4.5 | ^7.0",
        "symfony/phpunit-bridge": "^6.4.5 | ^7.0"
    },
    "autoload": {
        "psr-4": {
            "SymfonyCasts\\Bundle\\VerifyEmail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonyCasts\\Bundle\\VerifyEmail\\Tests\\": "tests/"
        }
    }
}
